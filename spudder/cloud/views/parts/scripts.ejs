<script type="text/javascript">
    $(document).ready(function() {
        var logo = $('header .logo'),
            buttons = $('header #download-buttons'),
            hiddenTop = $('header .hidden-top');

        $('#header-hidden-link').find('.toggle-link').toggle(function(e) {
            e.preventDefault();
            buttons.css({
                'display' : 'table',
                'margin' : 'auto'
            });
            hiddenTop.css('margin-top', '0px');
            logo.css('margin-top', '5px');
        }, function() {
            hiddenTop.css('margin-top', '-5px');
            logo.css('margin-top', '-10px');
        });
    });
</script>
